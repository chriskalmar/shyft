{
  "name": "shift-engine",
  "version": "0.0.2",
  "description": "Model driven database and API generator",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --require test/setup.js -R spec --check-leaks test/*",
    "test:watch": "nodemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "cov": "NODE_ENV=test nyc npm run test",
    "coverage": "NODE_ENV=test nyc mocha --require test/setup.js -R spec --check-leaks  test/*",
    "coverage-detail": "NODE_ENV=test nyc --reporter=text mocha --require test/setup.js -R spec --check-leaks test/*",
    "coverage-ci": "NODE_ENV=test nyc mocha --require test/setup.js -R spec --check-leaks test/* && cat ./coverage/lcov.info | codecov",
    "babel": "babel --presets es2015 --ignore node_modules,coverage"
  },
  "lint-staged": {
    "*.js": "./node_modules/eslint/bin/eslint.js"
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "api",
    "postgres",
    "model",
    "generator"
  ],
  "author": "Chris Kalmar <christian.kalmar@gmail.com>",
  "contributors": [
  ],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 2.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.12.1",
    "eslint-plugin-mocha": "^4.7.0",
    "lint-staged": "^3.2.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "pre-commit": "^1.2.1"
  },
  "dependencies": {
    "ajv": "^4.10.0",
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "chalk": "^1.1.3",
    "lodash": "^4.17.2",
    "marko": "^3.13.0"
  }
}
