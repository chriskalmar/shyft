{
  "name": "shift-graphql",
  "version": "0.0.1",
  "description": "GraphQL extension for shift",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --require test/setup.js -R spec --check-leaks **/*.js",
    "test:watch": "nodemon",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "cov": "NODE_ENV=test nyc npm run test",
    "coverage": "NODE_ENV=test nyc --require test/setup.js -R spec --check-leaks **/*.js",
    "coverage-detail": "NODE_ENV=test nyc --reporter=text mocha --require test/setup.js -R spec --check-leaks **/*.js",
    "coverage-ci": "NODE_ENV=test nyc mocha --require test/setup.js -R spec --check-leaks **/*.js && cat ./coverage/lcov.info | codecov",
    "babel": "babel --presets es2015,stage-0 --ignore node_modules,coverage"
  },
  "lint-staged": {
    "*.js": "./node_modules/eslint/bin/eslint.js"
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "graphql",
    "shift",
    "model",
    "generator"
  ],
  "author": "Chris Kalmar <christian.kalmar@gmail.com>",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 2.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-mocha": "^4.9.0",
    "graphql": "^0.9.3",
    "graphql-relay": "^0.5.1",
    "graphql-tools": "^0.11.0",
    "lint-staged": "^3.4.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.2.0",
    "pluralize": "^4.0.0",
    "pre-commit": "^1.2.2"
  }
}
